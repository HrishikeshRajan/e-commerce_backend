{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript","security/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": {
          "allowExpressions": true
        }
      }
    }
